{
  "name": "qrgen-extension",
  "version": "1.0.1",
  "description": "A browser extension which lets you create short URLs and QR-Codess",
  "permissions": [
    "https://qrgen.cc/*",
    "activeTab"
  ],
  "browser_action": {
      "default_title": "Create QR-Code",
      "default_popup": "popup/popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "MacCtrl+Q"
      }
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "manifest_version": 2
}
